
  {
    "manifest_version": 3,
    "name": "GrammerAI",
    "version": "1.0.0",    
    "description": "A browser extension to interact with AI providers like OpenAI, Anthropic, and Together",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icons/icon-48.png"
      }
    },
    "icons": {
      "48": "icons/icon-48.png"
    },
    "permissions": ["storage", "activeTab"]
  }
  